{

  "manifest_version": 2,
  "name": "Vog",
  "version": "1.0",

  "description": "View OpenGraph data conveniently with a browser extension.",
  "homepage_url": "https://github.com/milesmcc/vog",

  "icons": {
    "48": "icons/extract.svg",
    "96": "icons/extract.svg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "icons/extract.svg",
    "default_title": "Extract",
    "default_popup": "popup/extract.html"
  }

}
